<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="plugin.video.themoviedb.helper"
version="0.1.0"
name="TheMovieDb Helper"
provider-name="jurialmunkey">
<requires>
  <import addon="xbmc.python" version="2.25.0"/>
  <import addon="script.module.simplecache" version="1.0.14" />
  <import addon="script.module.requests" version="2.9.1"/>
</requires>
<extension point="xbmc.python.pluginsource" library="main.py">
  <provides>video</provides>
</extension>
<extension point="xbmc.addon.metadata">
  <summary lang="en">TheMovieDb Helper</summary>
  <description lang="en_GB">TheMovieDb Helper provides skinners a number of ways to access TheMovieDb API</description>
  <disclaimer lang="en_GB">Provided by TheMovieDb. Please make sure you have your own API key.</disclaimer>
  <assets>
    <icon>icon.png</icon>
    <fanart>fanart.jpg</fanart>
    <screenshot>resources\screenshot-01.jpg</screenshot>
  </assets>
</extension>
</addon>

